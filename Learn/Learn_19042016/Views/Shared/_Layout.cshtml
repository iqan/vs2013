<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
</head>

<body>
    <div class="page">

        <div id="header">
            <div id="title">
                <h1>
                    This is header.
                </h1>
            </div>

            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>

            <div id="menucontainer">

                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact Us", "ContactUs", "Home")</li>
                </ul>
            </div>
        </div>

        <div id="main">
            @RenderBody()
            <div id="footer">

            </div>
        </div>
    </div>
</body>
</html>